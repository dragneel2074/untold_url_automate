    "execution": {
     "iopub.execute_input": "2025-05-22T04:25:24.101359Z",
     "iopub.status.busy": "2025-05-22T04:25:24.100970Z",
     "iopub.status.idle": "2025-05-22T04:25:24.113978Z",
     "shell.execute_reply": "2025-05-22T04:25:24.112828Z"
