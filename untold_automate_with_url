    "# import json\n",
    "# import time\n",
    "# import requests\n",
    "# # from groq import Groq\n",
    "# import google.generativeai as genai\n",
    "\n",
    "# GLAMA_BASE_URL = \"https://glama.ai/api/gateway/openai/v1\"\n",
    "# GLAMA_PRIMARY_MODEL = \"gemini-2.0-flash-thinking-exp-01-21\" # Example model routing via Glama\n",
    "\n",
    "# glama_client = OpenAI(\n",
    "#     base_url=GLAMA_BASE_URL,\n",
    "#     api_key=glama_api\n",
    "# )\n",
    "\n",
    "# # Configure Gemini client\n",
    "# genai.configure(api_key=gemini_api)\n",
    "# gemini_model = genai.GenerativeModel(\n",
    "#     model_name=\"gemini-1.5-flash\", # Use the same model as before\n",
    "#     generation_config={\n",
    "#         \"temperature\": 1,\n",
    "#         \"top_p\": 0.95,\n",
    "#         \"top_k\": 64,\n",
    "#         \"max_output_tokens\": 8192,\n",
    "#     }\n",
    "# )\n",
    "\n",
    "# # Removed translate_with_pollinations as it's no longer used\n",
    "\n",
    "# def translate_task(start_chapter=1, end_chapter=None):\n",
    "#     \"\"\"\n",
    "#     Translate Chinese chapters to English using multiple APIs with fallback:\n",
    "#     1. Glama (via OpenAI SDK)\n",
    "#     2. Google Gemini (Directly)\n",
    "#     \"\"\"\n",
    "#     # Load data and configuration\n",
    "#     try:\n",
    "#         with open(SPLIT_CHAPTERS_FILE, 'r', encoding='utf-8') as f:\n",
    "#             book_data = json.load(f)\n",
    "#     except FileNotFoundError:\n",
    "#         print(f\"Error: {SPLIT_CHAPTERS_FILE} not found.\")\n",
    "#         return\n",
    "        \n",
    "#     try:\n",
    "#         with open(GLOSSARY_FILE, 'r', encoding='utf-8') as f:\n",
    "#             glossary_list = json.load(f) # Load as list\n",
    "#         formatted_glossary = \"\\n\".join(glossary_list) # Format as string\n",
    "#     except FileNotFoundError:\n",
    "#         print(f\"Error: {GLOSSARY_FILE} not found. Proceeding without glossary.\")\n",
    "#         formatted_glossary = \"\"\n",
    "#     except Exception as e:\n",
    "#         print(f\"Error loading glossary: {e}. Proceeding without glossary.\")\n",
    "#         formatted_glossary = \"\"\n",
    "\n",
    "#     translations = []\n",
    "\n",
    "#     # Select chapters to translate\n",
