    "#              print(translation) # Print the failure message\n",
    "#         print('=======================================')\n",
    "\n",
    "#         # Sleep between chapter requests to avoid hitting rate limits immediately\n",
    "#         # Adjust the sleep time as needed\n",
    "#         time.sleep(5)\n",
    "\n",
    "#     # Save all translations\n",
    "#     print(f'Saving translations to {TRANSLATIONS_FILE}')\n",
