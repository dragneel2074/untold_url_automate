   "source": [
    "def create_post(title, content, slug, username, password, scheduled_date, category_id):\n",
    "   post_data = {\n",
    "       'title': title,\n",
    "       'content': content,\n",
    "       'status': 'future',\n",
    "       'date': scheduled_date.isoformat(),\n",
    "       'slug': slug,\n",
    "       'categories': [category_id],\n",
    "       'meta': {\n",
    "           'rank_math_focus_keyword': title.replace(',', '')\n",
    "       }\n",
    "   }\n",
    "   response = requests.post(\n",
    "       f'{API_URL}/posts',\n",
    "       auth=(username, password),\n",
    "       json=post_data\n",
    "   )\n",
    "   return response.json()\n"
   ]
  },
