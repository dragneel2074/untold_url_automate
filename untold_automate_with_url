    "            }\n",
    "        ]\n",
    "    }\n",
    "}\n",
    "\n",
    "async def scrape_site_iteratively(first_url, final_url=None, ua=None):\n",
    "    ua = ua or UserAgent()\n",
    "    chapters = []\n",
    "    current_url = first_url\n",
    "\n",
    "    async with AsyncWebCrawler() as crawler:\n",
    "        while current_url and (not final_url or current_url != final_url):\n",
    "            domain = next((d for d in domain_selectors if d in current_url), None)\n",
    "            if not domain:\n",
    "                print(f\"No selectors defined for domain in URL {current_url}, stopping.\")\n",
    "                break\n",
    "            # sel = domain_selectors[domain]\n",
    "            # schema = domain_selectors[domain]\n",
    "            schema = domain_selectors[domain]\n",
    "            # schema = {\n",
    "            #     \"name\": \"ChapterExtractor\",\n",
    "            #     \"baseSelector\": \"body\",  # root of scraping\n",
    "            #     \"fields\": [\n",
    "            #         {\"name\": \"title\", \"selector\": sel[\"title\"], \"type\": \"text\"},\n",
    "            #         {\"name\": \"content\", \"selector\": sel[\"content\"], \"type\": \"text\"},\n",
    "            #         {\"name\": \"next_url\", \"selector\": sel[\"next_url\"], \"type\": \"attribute\", \"attribute\": \"href\"}\n",
    "            #     ]\n",
    "            # }\n",
    "            strategy = JsonCssExtractionStrategy(schema, verbose=False)\n",
    "\n",
    "            run_cfg = CrawlerRunConfig(\n",
    "                extraction_strategy=strategy,\n",
    "                user_agent=ua.random\n",
    "            )\n",
    "\n",
    "            print(f\"â¡ï¸ Crawling {current_url}\")\n",
    "            result = await crawler.arun(url=current_url, config=run_cfg)\n",
    "            time.sleep(1)\n",
    "            if not result.success:\n",
    "                print(f\"âŒ Failed: {result.error_message}\")\n",
    "                break\n",
    "\n",
    "            try:\n",
    "                print(result.markdown[:200])\n",
    "                data = json.loads(result.extracted_content)\n",
    "                if not data:\n",
    "                    print(\"âš ï¸ No extracted JSON; stopping.\")\n",
    "                    break\n",
    "                item = data[0]\n",
    "                title = item.get(\"title\", \"\").strip()\n",
    "                content = item.get(\"content\", \"\").strip()\n",
    "                next_href = item.get(\"next_url\")\n",
    "            except Exception as e:\n",
    "                print(f\"ğŸ”¥ Parsing error: {e}\")\n",
    "                break\n",
    "\n",
    "            if title and content:\n",
    "                chapters.append({\"title\": title, \"content\": content, \"url\": result.url})\n",
    "                print(f\"âœ… Chapter {len(chapters)}: {title}\")\n",
    "            else:\n",
    "                print(\"âš ï¸ Missing title/content; stopping.\")\n",
    "                break\n",
    "\n",
    "            if next_href:\n",
    "                current_url = urljoin(current_url, next_href)\n",
    "                if final_url and current_url == final_url:\n",
    "                    print(\"ğŸ¯ Reached final URL; stopping.\")\n",
    "                    break\n",
    "                await asyncio.sleep(random.uniform(2, 5))\n",
    "            else:\n",
    "                print(\"ğŸš« No next link; stopping.\")\n",
    "                break\n",
    "\n",
    "    return chapters\n",
    "\n",
    "async def main_scrape(first_url, final_url=None):\n",
    "    ua = UserAgent()\n",
    "    return await scrape_site_iteratively(first_url, final_url, ua)\n",
    "\n",
    "# if __name__ == \"__main__\":\n",
    "#     first = \"https://www.banxia.la/books/143300/28251886.html\"\n",
    "#     last = \"https://www.banxia.la/books/143300/28251912.html\"\n",
    "#     chapters = asyncio.run(main_scrape(first, last))\n",
    "#     print(f\"\\nâœ… Done: {len(chapters)} chapters scraped.\")\n"
   ]
  },
