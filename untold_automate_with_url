   "cell_type": "code",
   "execution_count": 22,
   "id": "1ddcc151",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-05-29T15:52:53.468614Z",
     "iopub.status.busy": "2025-05-29T15:52:53.468186Z",
