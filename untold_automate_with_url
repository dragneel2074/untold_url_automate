  {
   "cell_type": "code",
   "execution_count": 16,
   "id": "2749a043",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-05-23T02:17:12.447074Z",
     "iopub.status.busy": "2025-05-23T02:17:12.446724Z",
     "iopub.status.idle": "2025-05-23T02:17:14.015689Z",
     "shell.execute_reply": "2025-05-23T02:17:14.014757Z"
    },
    "papermill": {
     "duration": 1.621949,
     "end_time": "2025-05-23T02:17:14.018055",
     "exception": false,
     "start_time": "2025-05-23T02:17:12.396106",
     "status": "completed"
