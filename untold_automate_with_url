  {
   "cell_type": "code",
   "execution_count": 16,
   "id": "513bcdda",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-05-22T04:25:24.828108Z",
     "iopub.status.busy": "2025-05-22T04:25:24.827595Z",
     "iopub.status.idle": "2025-05-22T04:25:26.462866Z",
     "shell.execute_reply": "2025-05-22T04:25:26.461733Z"
    },
    "papermill": {
     "duration": 1.689292,
     "end_time": "2025-05-22T04:25:26.465447",
     "exception": false,
     "start_time": "2025-05-22T04:25:24.776155",
     "status": "completed"
    },
    "tags": []
