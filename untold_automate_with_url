    "import requests\n",
    "import json\n",
    "from datetime import datetime, timedelta\n",
    "import pytz\n",
    "# Payload for creating the category\n",
    "data = {\n",
    "    \"name\": novel_title,\n",
    "    \"slug\": novel_slug,\n",
    "    \"description\": \"\"\n",
    "}\n",
    "\n",
    "# Make the POST request to create the category\n",
    "response = requests.post(Category_API_URL, \n",
    "                         data=json.dumps(data), \n",
    "                         headers={'Content-Type': 'application/json'},\n",
    "                         auth=HTTPBasicAuth(USERNAME, PASSWORD))\n",
    "\n",
    "# Check the response\n",
    "if response.status_code == 201:\n",
    "    print(\"Category created successfully:\", response.json())\n",
    "else:\n",
    "    print(\"Failed to create category. Status Code:\", response.status_code)\n",
    "    print(\"Response:\", response.text)\n"
