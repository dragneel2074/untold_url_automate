   "execution_count": 12,
   "id": "45286959",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-05-23T02:17:11.681367Z",
     "iopub.status.busy": "2025-05-23T02:17:11.680317Z",
     "iopub.status.idle": "2025-05-23T02:17:11.693438Z",
     "shell.execute_reply": "2025-05-23T02:17:11.692418Z"
    },
    "papermill": {
     "duration": 0.065341,
     "end_time": "2025-05-23T02:17:11.695370",
     "exception": false,
     "start_time": "2025-05-23T02:17:11.630029",
     "status": "completed"
