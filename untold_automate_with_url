    "#     },\n",
    "#     \"ddxs.com\": {\n",
    "#         \"name\": \"ddxs\",\n",
    "#         \"baseSelector\": \"body\",\n",
    "#         \"fields\": [\n",
    "#             {\"name\": \"title\", \"selector\": \"h1, .nr_title\", \"type\": \"text\"},\n",
    "#             {\"name\": \"content\", \"selector\": \"#contents, #nr1\", \"type\": \"text\"},\n",
    "#             {\"name\": \"next_url\", \"selector\": \"#footlink a:nth-child(3), #pb_next\", \"type\": \"attribute\", \"attribute\": \"href\"}\n",
    "#         ]\n",
    "#     },\n",
    "#     \"69shuba.com\": {\n",
    "#         \"name\": \"69shuba\",\n",
    "#         \"baseSelector\": \"body\",\n",
    "#         \"fields\": [\n",
    "#             {\"name\": \"title\", \"selector\": \"h1.hide720\", \"type\": \"text\"},\n",
    "#             {\"name\": \"content\", \"selector\": \"div.txtnav\", \"type\": \"text\"},\n",
    "#             {\"name\": \"next_url\", \"selector\": \"div.page1 a:last-child\", \"type\": \"attribute\", \"attribute\": \"href\"}\n",
    "#         ]\n",
    "#     },\n",
    "#     \"banxia.la\": {\n",
    "#         \"name\": \"banxia\",\n",
    "#         \"baseSelector\": \"body\",\n",
    "#         \"fields\": [\n",
    "#             {\"name\": \"title\", \"selector\": \"#nr_title\", \"type\": \"text\"},\n",
    "#             {\"name\": \"content\", \"selector\": \"#nr1\", \"type\": \"text\"},\n",
    "#             {\"name\": \"next_url\", \"selector\": \".nav2 .next a\", \"type\": \"attribute\", \"attribute\": \"href\"}\n",
    "#         ]\n",
    "#     },\n",
    "#     \"22shuqu.com\": {\n",
    "#         \"name\": \"22shuqu\",\n",
    "#         \"baseSelector\": \"body\",\n",
    "#         \"fields\": [\n",
    "#             {\"name\": \"title\", \"selector\": \"h1.title\", \"type\": \"text\"},\n",
    "#             {\"name\": \"content\", \"selector\": \"div.content\", \"type\": \"text\"},\n",
    "#             {\"name\": \"next_url\", \"selector\": \"#next_url\", \"type\": \"attribute\", \"attribute\": \"href\"}\n",
    "#         ]\n",
    "#     }\n",
    "# }\n",
    "\n",
    "domain_selectors={\n",
    "    \"uukanshu.cc\": {\n",
    "        \"name\": \"uukanshu\",\n",
    "        \"baseSelector\": \"body\",\n",
    "        \"fields\": [\n",
    "            {\n",
    "                \"name\": \"title\",\n",
    "                \"selector\": \"h1.pt10\",\n",
    "                \"type\": \"text\"\n",
    "            },\n",
    "            {\n",
    "                \"name\": \"content\",\n",
    "                \"selector\": \"div.readcotent.bbb.font-normal\",\n",
    "                \"type\": \"text\"\n",
    "            },\n",
    "            {\n",
    "                \"name\": \"next_url\",\n",
    "                \"selector\": \"#linkNext\",\n",
    "                \"type\": \"attribute\",\n",
    "                \"attribute\": \"href\"\n",
    "            }\n",
    "        ]\n",
    "    },\n",
    "    \"ddxs.com\": {\n",
    "        \"name\": \"ddxs\",\n",
    "        \"baseSelector\": \"body\",\n",
    "        \"fields\": [\n",
    "            {\n",
    "                \"name\": \"title\",\n",
    "                \"selector\": \"h1, .nr_title\",\n",
    "                \"type\": \"text\"\n",
    "            },\n",
    "            {\n",
    "                \"name\": \"content\",\n",
    "                \"selector\": \"#contents, #nr1\",\n",
    "                \"type\": \"text\"\n",
    "            },\n",
    "            {\n",
    "                \"name\": \"next_url\",\n",
    "                \"selector\": \"#footlink a:nth-child(3), #pb_next\",\n",
    "                \"type\": \"attribute\",\n",
    "                \"attribute\": \"href\"\n",
    "            }\n",
    "        ]\n",
    "    },\n",
    "    \"69shuba.com\": {\n",
    "        \"name\": \"69shuba\",\n",
    "        \"baseSelector\": \"body\",\n",
    "        \"fields\": [\n",
    "            {\n",
    "                \"name\": \"title\",\n",
