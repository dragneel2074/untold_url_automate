   "cell_type": "code",
   "execution_count": 16,
   "id": "87561aef",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-05-29T15:45:42.226180Z",
     "iopub.status.busy": "2025-05-29T15:45:42.225805Z",
