   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-05-22T04:28:10.668076Z",
     "iopub.status.busy": "2025-05-22T04:28:10.667711Z",
     "iopub.status.idle": "2025-05-22T04:28:10.688021Z",
