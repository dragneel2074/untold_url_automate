   "cell_type": "code",
   "execution_count": 16,
   "id": "8bde29bb",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-06-09T10:05:26.255695Z",
     "iopub.status.busy": "2025-06-09T10:05:26.255224Z",
     "iopub.status.idle": "2025-06-09T10:05:28.083423Z",
     "shell.execute_reply": "2025-06-09T10:05:28.082148Z"
    },
    "papermill": {
     "duration": 1.874813,
     "end_time": "2025-06-09T10:05:28.086019",
     "exception": false,
     "start_time": "2025-06-09T10:05:26.211206",
