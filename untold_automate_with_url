    "                \"name\": \"content\",\n",
    "                \"selector\": \"div.txtnav\",\n",
    "                \"type\": \"text\"\n",
    "            },\n",
    "            {\n",
    "                \"name\": \"next_url\",\n",
    "                \"selector\": \"div.page1 a:last-child\",\n",
    "                \"type\": \"attribute\",\n",
    "                \"attribute\": \"href\"\n",
    "            }\n",
    "        ]\n",
    "    },\n",
    "    \"banxia.la\": {\n",
    "        \"name\": \"banxia\",\n",
    "        \"baseSelector\": \"body\",\n",
    "        \"fields\": [\n",
    "            {\n",
    "                \"name\": \"title\",\n",
    "                \"selector\": \"#nr_title\",\n",
    "                \"type\": \"text\"\n",
    "            },\n",
    "            {\n",
    "                \"name\": \"content\",\n",
