   "execution_count": 22,
   "id": "a89216a8",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-08-03T17:05:57.098626Z",
     "iopub.status.busy": "2025-08-03T17:05:57.098263Z",
     "iopub.status.idle": "2025-08-03T17:05:57.119655Z",
     "shell.execute_reply": "2025-08-03T17:05:57.118809Z"
    },
    "papermill": {
     "duration": 0.194402,
     "end_time": "2025-08-03T17:05:57.121526",
     "exception": false,
     "start_time": "2025-08-03T17:05:56.927124",
     "status": "completed"
