  {
   "cell_type": "code",
   "execution_count": 17,
   "id": "d1029559",
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-08-03T17:00:53.868285Z",
     "iopub.status.busy": "2025-08-03T17:00:53.867946Z",
     "iopub.status.idle": "2025-08-03T17:00:57.539891Z",
     "shell.execute_reply": "2025-08-03T17:00:57.538327Z"
    },
    "papermill": {
     "duration": 3.808933,
     "end_time": "2025-08-03T17:00:57.541589",
     "exception": false,
     "start_time": "2025-08-03T17:00:53.732656",
     "status": "completed"
    },
    "tags": []
   },
   "outputs": [],
   "source": [
    "import random\n",
    "import json\n",
    "import time\n",
    "import google.generativeai as genai\n",
    "from openai import OpenAI # Import the OpenAI SDK for Glama\n",
    "\n",
    "\n",
    "# Configure Glama client (using OpenAI SDK)\n",
    "GLAMA_BASE_URL = \"https://glama.ai/api/gateway/openai/v1\"\n",
    "GLAMA_GLOSSARY_MODEL = \"gemini-2.0-flash-thinking-exp-01-21\" # Example model routing via Glama for glossary\n",
    "\n",
    "glama_client = OpenAI(\n",
    "    base_url=GLAMA_BASE_URL,\n",
    "    api_key=glama_api\n",
    ")\n",
    "\n",
    "# Configure Gemini client\n",
    "genai.configure(api_key=gemini_api)\n",
    "gemini_model = genai.GenerativeModel(\n",
    "    model_name=\"gemini-1.5-flash\", # You can choose a different Gemini model if needed\n",
    "    generation_config={\n",
    "        \"temperature\": 0.5, # Lower temperature might be better for glossary extraction\n",
    "        \"top_p\": 0.9,\n",
    "        \"top_k\": 32,\n",
    "        \"max_output_tokens\": 2048, # Adjust token limit as needed\n",
    "    }\n",
    ")\n",
    "\n",
    "def get_api_response(api_choice, prompt, model=None):\n",
    "    \"\"\"Helper function to get API response from Glama or Gemini.\"\"\"\n",
    "    max_retries = 3\n",
    "    retry_count = 0\n",
    "    while retry_count < max_retries:\n",
    "        try:\n",
    "            if api_choice == 'glama':\n",
    "                if not model:\n",
    "                     raise ValueError(\"Model must be specified for Glama API.\")\n",
    "                response = glama_client.chat.completions.create(\n",
    "                    messages=[{\"role\": \"user\", \"content\": prompt}],\n",
    "                    model=model,\n",
    "                    timeout=60\n",
    "                )\n",
    "                return response.choices[0].message.content.strip()\n",
    "            elif api_choice == 'gemini':\n",
    "                response = gemini_model.generate_content(prompt)\n",
    "                return response.text.strip()\n",
    "            else:\n",
    "                raise ValueError(\"Invalid api_choice. Choose 'glama' or 'gemini'.\")\n",
    "\n",
    "        except Exception as e:\n",
    "            retry_count += 1\n",
    "            if retry_count < max_retries:\n",
    "                print(f\"API Error ({api_choice}) (Attempt {retry_count}/{max_retries}): {str(e)}\")\n",
    "                print(f\"Retrying in 30 seconds...\")\n",
    "                time.sleep(30)\n",
    "            else:\n",
    "                print(f\"API ({api_choice}) failed after {max_retries} attempts: {str(e)}\")\n",
    "                return None # Return None if all retries fail\n",
    "\n",
    "def create_glossary(api_choice='glama'):\n",
    "    \"\"\"\n",
    "    Create a glossary from random chapters and refine it using the specified API.\n",
    "\n",
    "    Args:\n",
    "        api_choice (str): The API to use for glossary creation and refinement.\n",
    "                          Choose 'glama' or 'gemini'.\n",
    "    \"\"\"\n",
    "    if api_choice.lower() not in ['glama', 'gemini']:\n",
    "        print(\"Invalid api_choice. Choose 'glama' or 'gemini'. Defaulting to 'glama'.\")\n",
    "        api_choice = 'glama'\n",
    "\n",
    "    print(f\"Using {api_choice.capitalize()} for glossary creation and refinement.\")\n",
    "\n",
    "    try:\n",
    "        with open(SPLIT_CHAPTERS_FILE, 'r', encoding='utf-8') as f:\n",
    "            book_data = json.load(f)  # book_data is a list of chapters\n",
    "    except FileNotFoundError:\n",
    "        print(f\"Error: {SPLIT_CHAPTERS_FILE} not found.\")\n",
    "        return\n",
    "\n",
    "    # Select 20 random chapters from the first 100 chapters\n",
    "    # Ensure we don't request more chapters than available\n",
    "    num_chapters_to_sample = max(1, int(len(book_data) * 0.1))\n",
    "    if num_chapters_to_sample == 0:\n",
    "        print(\"No chapters found in book data.\")\n",
    "        return\n",
    "\n",
    "    random_chapters = random.sample(book_data, num_chapters_to_sample)\n",
    "\n",
    "    preliminary_glossary = []\n",
    "\n",
    "    print(f\"Extracting preliminary glossary terms from {num_chapters_to_sample} random chapters...\")\n",
    "    for i, chapter in enumerate(random_chapters):\n",
    "        chapter_title = chapter.get('title', f'Chapter {i + 1}')\n",
    "        chapter_content = chapter.get('content', '')\n",
    "\n",
    "        if not chapter_content:\n",
    "            print(f\"Skipping empty content for {chapter_title}\")\n",
    "            continue\n",
    "\n",
    "        prompt = f\"\"\"Analyze the following Chinese web novel chapter and extract 15 to 20 important terms or names for a glossary. Each entry should include the Chinese term and its English equivalent or explanation. Translate character names (keeping Pinyin for the name part, e.g., Zhu Shijiu), locations names, unique concepts, cultivation levels, power levels, power techniques, or culturally specific terms to English.\n",
    "The target audience are people from USA that don't know much about Chinese language and culture.\n",
    "Very important Note: Only Use Pinyin for the given Character's Name. All other non-name terms must be translated to English. Do not use Pinyin for concepts, items, locations, etc.\n",
    "\n",
    "Chinese chapter:\n",
    "{chapter_content}\n",
    "\n",
    "Create a glossary of important terms in the following format, with one entry per line:\n",
    "Chinese Term: English Equivalent or Explanation\n",
    "for example:\n",
    "朱士久 : Zhu Shijiu (he)\n",
    "贝蒂 : Betty (she)\n",
    "白家 : Bai Family\n",
    "成长系统 : Growth System\n",
    "\n",
    "Provide only the glossary entries, with no extra text, introduction, or conclusion.\n",
    "\n",
    "Note: \n",
    "\n",
    "{glossary_note}\n",
    "\n",
    "\"\"\"\n",
    "\n",
    "        print(f\"Extracting terms for {chapter_title} using {api_choice.capitalize()}...\")\n",
    "        \n",
    "        if api_choice == 'glama':\n",
    "            chapter_glossary_text = get_api_response('glama', prompt, model=GLAMA_GLOSSARY_MODEL)\n",
    "        else: # api_choice == 'gemini'\n",
    "            chapter_glossary_text = get_api_response('gemini', prompt)\n",
    "\n",
    "        if chapter_glossary_text:\n",
    "            # Split by newline and clean up empty lines\n",
    "            entries = [entry.strip() for entry in chapter_glossary_text.split('\\n') if entry.strip()]\n",
    "            preliminary_glossary.extend(entries)\n",
    "            print(f\"Extracted {len(entries)} terms for {chapter_title}.\")\n",
    "        else:\n",
    "            print(f\"Failed to extract terms for {chapter_title}.\")\n",
    "\n",
    "        time.sleep(5) # Small delay between chapter processing\n",
    "\n",
    "    print(f\"\\nPreliminary glossary created with {len(preliminary_glossary)} raw entries.\")\n",
    "    if not preliminary_glossary:\n",
    "        print(\"No terms were extracted. Cannot refine the glossary.\")\n",
    "        return\n",
    "\n",
    "    # Refine the glossary\n",
    "    print(f\"Refining the glossary using {api_choice.capitalize()}...\")\n",
    "    glossary_joined = '\\n'.join(preliminary_glossary)\n",
    "    refine_prompt = f\"\"\"Refine and consolidate the following list of glossary entries for a Chinese web novel.\n",
    "1. Remove duplicates and redundant entries.\n",
    "2. Combine similar entries if appropriate (e.g., different forms of the same name/term).\n",
    "3. Ensure consistency in naming and explanations include pronouns in brackets like 贝蒂 : Betty (she).\n",
    "4. Retain people's names in Pinyin format (e.g., Chen Jingle), but fully translate all other terms, phrases, and concepts into English. Avoid using Pinyin for non-name elements to ensure clarity and natural flow for English readers.\n",
    "5. Format the final output strictly as a list of entries, one per line, in the format: \"Chinese Characters : English Equivalent\".\n",
    "\n",
    "Preliminary Glossary Entries (one per line):\n",
    "{glossary_joined}\n",
    "\n",
    "Refined Glossary (formatted as \"Chinese Characters : English Equivalent\"):\n",
    "\"\"\"\n",
    "    if api_choice == 'glama':\n",
    "        refined_glossary_text = get_api_response('glama', refine_prompt, model=GLAMA_GLOSSARY_MODEL)\n",
    "    else: # api_choice == 'gemini'\n",
    "        refined_glossary_text = get_api_response('gemini', refine_prompt)\n",
    "\n",
    "\n",
    "    if refined_glossary_text:\n",
    "        # Split by newline and clean up empty lines\n",
    "        refined_glossary = [entry.strip() for entry in refined_glossary_text.split('\\n') if entry.strip()]\n",
    "\n",
    "        # Save the refined glossary\n",
    "        print(f'Saving refined glossary to {GLOSSARY_FILE}')\n",
    "        try:\n",
    "            with open(GLOSSARY_FILE, 'w', encoding='utf-8') as f:\n",
    "                json.dump(refined_glossary, f, ensure_ascii=False, indent=2)\n",
    "            print(f'Glossary creation completed. Refined glossary saved to {GLOSSARY_FILE}')\n",
    "\n",
    "            # Read and print the final glossary\n",
    "            print(\"\\nFinal Refined Glossary Contents:\")\n",
    "            for term in refined_glossary:\n",
    "                print(term)\n",
    "\n",
    "        except Exception as e:\n",
    "            print(f\"Error saving refined glossary to {GLOSSARY_FILE}: {e}\")\n",
    "    else:\n",
    "        print(\"Failed to refine the glossary.\")\n",
    "\n",
    "# Usage Examples:\n",
    "# create_glossary(api_choice='glama') # Use Glama for glossary creation\n",
    "# create_glossary(api_choice='gemini') # Use Gemini for glossary creation\n",
    "\n",
    "# Make sure to replace placeholders GLAMA_API_KEY, GEMINI_API_KEY,\n",
    "# SPLIT_CHAPTERS_FILE, GLOSSARY_FILE with your actual values.\n",
    "# Also, verify GLAMA_GLOSSARY_MODEL is a valid model string for your Glama setup\n",
    "# and is suitable for this task."
   ]
  },
