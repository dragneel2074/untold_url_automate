    "                            return { title, content, nextUrl };\n",
    "                        }\n",
    "                    ''')\n",
    "                    if chapter_data['title'] and chapter_data['content']:\n",
    "                        chapters.append({\n",
    "                            'title': chapter_data['title'],\n",
    "                            'content': chapter_data['content'],\n",
    "                            'url': next_url\n",
    "                        })\n",
    "                    print(f\"Scraped chapter {chapter_count + 1}: {chapter_data['title']}\")\n",
    "                    next_url = chapter_data['nextUrl']\n",
