    "from openai import OpenAI\n",
    "\n",
    "# Suppress OpenAI SDK logs\n",
    "# logging.getLogger(\"openai\").setLevel(logging.ERROR)\n",
    "\n",
    "# Suppress httpx logs\n",
    "# logging.getLogger(\"httpx\").setLevel(logging.ERROR)\n",
    "\n",
    "client = OpenAI(\n",
    "    # OpenAI compatible API\n",
    "    base_url=\"https://glama.ai/api/gateway/openai/v1\",\n",
    "    # Your Glama Gateway API key\n",
    "    # https://glama.ai/settings/gateway/api-keys\n",
    "    api_key=glama_api\n",
    ")\n",
    "\n",
    "response = client.chat.completions.create(\n",
    "    messages=[\n",
    "        {\n",
    "            \"role\": \"user\",\n",
    "            \"content\": \"What's your name?\"\n",
    "        }\n",
    "    ],\n",
    "    # Let Glama choose the best provider\n",
    "    model=\"gemini-2.0-flash-thinking-exp-01-21\",\n",
    "    \n",
    "    \n",
    "    # or specify a specific provider...\n",
    "    # model=\"google-vertex/gemini-2.0-flash-exp\"\n",
    ")\n",
    "\n",
    "print(response.choices[0].message.content)\n"
   ]
