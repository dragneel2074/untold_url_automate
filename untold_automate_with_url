   "outputs": [],
   "source": [
    "def translate_task(start_chapter=1, end_chapter=None):\n",
    "    \"\"\"\n",
    "    Translate Chinese chapters to English using multiple APIs with fallback,\n",
    "    passing only chapter-relevant glossary terms.\n",
    "    1. Glama (via OpenAI SDK)\n",
    "    2. Google Gemini (Directly)\n",
    "    \"\"\"\n",
    "    # Load data and configuration\n",
    "    try:\n",
    "        with open(SPLIT_CHAPTERS_FILE, 'r', encoding='utf-8') as f:\n",
    "            book_data = json.load(f)\n",
    "    except FileNotFoundError:\n",
    "        print(f\"Error: {SPLIT_CHAPTERS_FILE} not found.\")\n",
    "        return\n",
    "        \n",
    "    glossary_list = [] # Initialize as an empty list\n",
    "    try:\n",
    "        with open(GLOSSARY_FILE, 'r', encoding='utf-8') as f:\n",
    "            glossary_list = json.load(f) # Load as list of strings\n",
    "        if not isinstance(glossary_list, list):\n",
    "            print(f\"Warning: Glossary file {GLOSSARY_FILE} does not contain a valid JSON list. Proceeding without glossary.\")\n",
    "            glossary_list = []\n",
    "    except FileNotFoundError:\n",
    "        print(f\"Warning: {GLOSSARY_FILE} not found. Proceeding without glossary.\")\n",
    "        # glossary_list remains []\n",
    "    except json.JSONDecodeError:\n",
    "        print(f\"Warning: Error decoding JSON from {GLOSSARY_FILE}. Proceeding without glossary.\")\n",
    "        # glossary_list remains []\n",
    "    except Exception as e:\n",
    "        print(f\"Error loading glossary: {e}. Proceeding without glossary.\")\n",
    "        # glossary_list remains []\n",
    "\n",
    "    translations = []\n",
    "\n",
    "    # Select chapters to translate\n",
    "    if end_chapter is not None:\n",
    "        chapters_to_translate = book_data[start_chapter-1:end_chapter]\n",
    "    else:\n",
    "        chapters_to_translate = book_data[start_chapter-1:]\n",
    "        end_chapter = len(book_data)\n",
    "\n",
    "    print(f\"Starting translation from Chapter {start_chapter} to {end_chapter}...\")\n",
    "    \n",
    "    for original_index, chapter in enumerate(chapters_to_translate, start=start_chapter-1):\n",
    "        current_chapter_number = original_index + 1\n",
    "        chapter_title = chapter.get('title', f'Chapter {current_chapter_number}')\n",
    "        chapter_content = chapter.get('content', '')\n",
    "\n",
    "        if not chapter_content:\n",
    "            print(f\"Skipping empty content for {chapter_title}\")\n",
    "            translations.append({\n",
    "                'title': chapter_title,\n",
    "                'translated_content': \"Content was empty.\"\n",
    "            })\n",
    "            continue\n",
    "\n",
    "        # --- MODIFICATION START: Filter glossary for relevant terms ---\n",
    "        relevant_glossary_for_chapter_entries = []\n",
    "        if glossary_list: # Only process if glossary_list has items\n",
    "            for entry_string in glossary_list:\n",
    "                if not isinstance(entry_string, str) or \" : \" not in entry_string:\n",
    "                    # print(f\"Warning: Skipping malformed glossary entry: {entry_string}\")\n",
    "                    continue\n",
    "                try:\n",
    "                    chinese_term = entry_string.split(\" : \", 1)[0].strip()\n",
    "                    if chinese_term and chinese_term in chapter_content: # Ensure chinese_term is not empty\n",
    "                        relevant_glossary_for_chapter_entries.append(entry_string)\n",
    "                except Exception as e:\n",
    "                    print(f\"Error processing glossary entry '{entry_string}': {e}\")\n",
    "        \n",
    "        chapter_specific_glossary_segment: str\n",
    "        if relevant_glossary_for_chapter_entries:\n",
    "            chapter_specific_glossary_segment = \"\\n\".join(relevant_glossary_for_chapter_entries)\n",
    "            print(f\"Found {len(relevant_glossary_for_chapter_entries)} relevant glossary terms for {chapter_title}.\")\n",
    "        else:\n",
    "            chapter_specific_glossary_segment = \"No specific glossary terms for this chapter.\"\n",
    "            # print(f\"No relevant glossary terms found for {chapter_title} (or glossary is empty/malformed).\")\n",
    "        # --- MODIFICATION END ---\n",
    "\n",
    "        # Build the translation prompt with the chapter-specific glossary\n",
    "        prompt = f\"\"\"Translate the following Chinese web novel chapter to English. Maintain the original tone and style of the novel. Preserve any cultural references or idioms, providing brief explanations in parentheses if necessary.\n",
    "If Paragraphs are stuck together, split them into separate paragraphs using double newlines (\\\\n\\\\n). Retain people's names in Pinyin format (e.g., Chen Jingle), but fully translate all other terms, phrases, and concepts into English. Avoid using Pinyin for non-name elements to ensure clarity and natural flow for English readers.\n",
    "You should translate every chinese character to English. The chapter should be fully translated.\n",
    "Use the following glossary for consistent translations of key terms:\n",
    "{chapter_specific_glossary_segment}\n",
    "\n",
    "Chinese chapter:\n",
    "{chapter_content}\n",
    "\n",
    "Note: Provide only the direct translation, no introductory sentences nor concluding sentences. Ensure no extra text is added before or after the translated chapter content.\n",
    "\n",
    "Sample Translation example:\n",
    "\n",
    "On a Certain Year, Month, Day, Hour, Minute, and Second.\n",
    "\n",
    "The world below remained as it always was—clear skies, no wind, and only a few thin clouds drifting by.\n",
    "\n",
    "Gao Wen quietly observed the distant land from an absolute top-down perspective, silently contemplating life—because, frankly, he had nothing else to do.\n",
    "\n",
    "He had long since lost track of how many years he'd remained in this state. He no longer knew what he looked like now. While he could roughly gauge time by the alternation of day and night, the truth was—after witnessing hundreds of thousands of such cycles—he had stopped bothering to count.\n",
    "\n",
    "This must be what people call transmigration, right?\n",
    "\n",
    "\"\"\"\n",
    "\n",
    "        translation = None\n",
    "        max_attempts = 2 \n",
    "        GLAMA_PRIMARY_MODEL = \"gemini-2.0-flash-thinking-exp-01-21\"\n",
    "        # 1. Try Glama (Primary)\n",
    "        print(f\"Attempting translation with Glama ({GLAMA_PRIMARY_MODEL})... (Chapter {current_chapter_number})\")\n",
    "        for attempt in range(max_attempts):\n",
    "            try:\n",
    "                response = glama_client.chat.completions.create(\n",
    "                    messages=[{\"role\": \"user\", \"content\": prompt}],\n",
    "                    model=GLAMA_PRIMARY_MODEL,\n",
    "                    timeout=60 \n",
    "                )\n",
    "                translation = response.choices[0].message.content.strip()\n",
    "                print(\"Glama translation successful.\")\n",
    "                break \n",
    "            except Exception as e:\n",
    "                print(f\"Glama error for {chapter_title} (attempt {attempt + 1}/{max_attempts}): {str(e)}\")\n",
    "                if attempt == max_attempts - 1:\n",
    "                    print(\"Glama failed after all attempts. Falling back to Gemini.\")\n",
    "                else:\n",
    "                    time.sleep(15) \n",
    "\n",
    "        # 2. If Glama failed, try Gemini (Secondary)\n",
    "        if not translation or \"TRANSLATION FAILED\" in translation:\n",
    "            print(f\"Attempting translation with Gemini... (Chapter {current_chapter_number})\")\n",
    "            for attempt in range(max_attempts):\n",
    "                try:\n",
    "                    gemini_response = gemini_model.generate_content(prompt)\n",
    "                    translation = gemini_response.text.strip()\n",
    "                    print(\"Gemini translation successful.\")\n",
    "                    break\n",
    "                except Exception as e:\n",
    "                    print(f\"Gemini error for {chapter_title} (attempt {attempt + 1}/{max_attempts}): {str(e)}\")\n",
    "                    if attempt == max_attempts - 1:\n",
    "                        print(f\"Failed to translate {chapter_title} after all attempts (Glama and Gemini).\")\n",
    "                        translation = f\"TRANSLATION FAILED for {chapter_title}: {str(e)}\"\n",
    "                    else:\n",
    "                        time.sleep(15)\n",
    "\n",
    "        if not translation:\n",
    "             translation = f\"TRANSLATION FAILED for {chapter_title}: Unknown error after all fallbacks.\"\n",
    "\n",
    "        translations.append({\n",
    "            'title': chapter_title,\n",
    "            'translated_content': translation\n",
    "        })\n",
    "        print(f\"Finished processing {chapter_title}\")\n",
    "        print(\"First 500 characters of translation:\")\n",
    "        if translation and \"TRANSLATION FAILED\" not in translation:\n",
    "             print(translation[:500]+ \"...\")\n",
    "        else:\n",
    "             print(translation)\n",
    "        print('=======================================')\n",
    "\n",
    "        time.sleep(5)\n",
    "\n",
    "    print(f'Saving translations to {TRANSLATIONS_FILE}')\n",
    "    try:\n",
    "        with open(TRANSLATIONS_FILE, 'w', encoding='utf-8') as f:\n",
    "            json.dump(translations, f, ensure_ascii=False, indent=2)\n",
    "        print(f'Translation completed. Translations saved to {TRANSLATIONS_FILE}')\n",
    "    except Exception as e:\n",
    "        print(f\"Error saving translations to {TRANSLATIONS_FILE}: {e}\")"
   ]
  },
